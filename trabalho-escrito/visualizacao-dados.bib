	Pages = {e20},
	Publisher = {JMIR Publications Inc.},
	Title = {Importance of Internet Surveillance in Public Health Emergency Control and Prevention: Evidence From a Digital Epidemiologic Study During Avian Influenza A H7N9 Outbreaks},
